function split(str)
	define arr as array
	define tmp as string
	foreach ch iterate str
		if(ch.isspace())
			if(size_of(tmp)!=0)
				arr.push_back(tmp)
				tmp.clear()
			end
			continue
		end
		tmp.append(ch)
	end
	if(size_of(tmp)!=0)
		arr.push_back(tmp)
		tmp.clear()
	end
	return arr
end
define r=split(system.getline())
for i=0 to size_of(r)-1
	system.print("#",r[i],"#")
end
system.println()
